## Generated by Ansible (roles/netbird-server/templates/setup.env.j2)
## Fill in the placeholder values (__FILL_ME__...) before running configure.sh manually if needed.

# Dashboard domain. e.g. app.mydomain.com
NETBIRD_DOMAIN="{{ netbird_domain }}"

# OIDC configuration e.g., https://example.eu.auth0.com/.well-known/openid-configuration
NETBIRD_AUTH_OIDC_CONFIGURATION_ENDPOINT="{{ netbird_auth_oidc_configuration_endpoint }}"

NETBIRD_AUTH_AUDIENCE="{{ netbird_auth_audience }}"
# e.g. netbird-client
NETBIRD_AUTH_CLIENT_ID="{{ netbird_auth_client_id }}"
NETBIRD_AUTH_CLIENT_SECRET="{{ netbird_auth_client_secret }}"

# indicates whether to use Auth0 or not: true or false
NETBIRD_USE_AUTH0="{{ netbird_use_auth0 | ternary('true', 'false') }}"

# Management / DNS
NETBIRD_MGMT_DNS_DOMAIN="{{ netbird_mgmt_dns_domain }}"
NETBIRD_MGMT_SINGLE_ACCOUNT_MODE_DOMAIN="{{ netbird_mgmt_single_account_mode_domain }}"

# Relay
NETBIRD_RELAY_ENDPOINT="{{ netbird_relay_endpoint }}"
NETBIRD_RELAY_AUTH_SECRET="{{ netbird_relay_auth_secret }}"

# TURN / coturn
TURN_DOMAIN="{{ netbird_turn_domain }}"
TURN_MIN_PORT="{{ netbird_turn_min_port }}"
TURN_MAX_PORT="{{ netbird_turn_max_port }}"

# Tags
NETBIRD_MANAGEMENT_TAG="{{ netbird_management_tag }}"
NETBIRD_DASHBOARD_TAG="{{ netbird_dashboard_tag }}"
NETBIRD_SIGNAL_TAG="{{ netbird_signal_tag }}"
NETBIRD_RELAY_TAG="{{ netbird_relay_tag }}"
COTURN_TAG="{{ netbird_coturn_tag }}"
